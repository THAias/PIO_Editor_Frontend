{
  "name": "pio_editor_frontend",
  "version": "1.2.1",
  "description": "",
  "main": "index.tsx",
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm run prettier:fix && cross-env NODE_ENV=development REACT_APP_VERSION_ENV=webVersion PUBLIC_URL=/ react-scripts start",
    "build": "tsc -p tsconfig.json --noEmit && PUBLIC_URL=editor/ GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "build-local": "tsc -p tsconfig.json --noEmit && cross-env PUBLIC_URL=/ REACT_APP_VERSION_ENV=webVersion GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "start:localVersion": "npm run prettier:fix && cross-env NODE_ENV=development REACT_APP_VERSION_ENV=localVersion PUBLIC_URL=/ react-scripts start",
    "prettier:fix": "prettier --write \"{src,tests}/**/*.{js,ts,tsx,jsx,css,sass,scss,less,html,md}\"",
    "prettier:check": "prettier --check \"{src,tests}/**/*.{js,ts,tsx,jsx,css,sass,scss,less,html,md}\"",
    "lint": "tsc --noEmit && eslint --fix \"{src,tests}/**/*.{ts,tsx}\"",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:sec": "eslint {src,tests}/**/*.ts -f json -o eslint-sast-report.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@fontsource/roboto": "^5.0.14",
    "@reduxjs/toolkit": "^1.9.7",
    "@thaias/pio_editor_meta": "1.1.9",
    "@thaias/pio_fhir_resources": "^2.3.20",
    "antd": "5.20.0",
    "axios": "^1.7.3",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.12",
    "js-file-download": "^0.4.12",
    "rc-field-form": "^2.2.1",
    "rc-upload": "^4.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-fast-marquee": "^1.6.5",
    "react-redux": "^8.1.3",
    "react-scripts": "^5.0.1",
    "react-toastify": "^10.0.5",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/events": "^3.0.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.33",
    "@types/redux-logger": "3.0.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "csstype": "^3.1.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.7.0",
    "eslint-plugin-jsdoc": "^50.0.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^1.0.4",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^15.2.8",
    "npm-check-updates": "^17.0.3",
    "prettier": "3.3.3",
    "prop-types": "^15.8.1",
    "sass": "^1.77.8",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "4.9.5"
  },
  "overrides": {
    "nth-check": "2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.{ts,tsx,js,json,css,sass,scss,less,html,md}": "npm run prettier:fix",
    "{src,tests}/**/*.{ts,tsx}": "npm run lint --fix"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
